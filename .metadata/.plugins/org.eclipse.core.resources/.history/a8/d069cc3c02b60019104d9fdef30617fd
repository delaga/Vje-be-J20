import stanford.karel.Karel;


public class OurKarelProgram extends Karel{
	public void run(){
		move();
		move();
		turnRight();
		move();
		turnLeft();
		move();
		pickBeeper();
		move();
		putBeeper();
		turnRight();
		move();
		move();
		turnAround();
	}
	private void turnRight(){
		turnLeft();
		turnLeft();
		turnLeft();
	}
	private void turnAround(){
		turnLeft();
		turnLeft();
	}
	
}
